{
    "name": "dminustin/laravel-api-factory",
    "minimum-stability": "stable",
    "readme": "README.md",
    "description": "All-in-one Factory helps to create API and docs",
    "version": "1.1.0.7",
    "keywords": [
        "laravel",
        "api",
        "routes",
        "controllers generator",
        "swagger",
        "postman collection",
        "laravel-api-factory"
    ],
    "homepage": "https://github.com/dminustin/laravel-api-factory",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Danila Minustin",
            "email": "danila.minustin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "8.0|^8.0",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/filesystem": ">=5.1",
        "illuminate/console": ">=5.1",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.18",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dminustin\\ApiFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dminustin\\ApiFactory\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dminustin\\ApiFactory\\LaravelApiFactoryServiceProvider"
            ],
            "aliases": {
                "ApiFactory": "Dminustin\\ApiFactory\\LaravelApiFactoryFacade"
            }
        }
    }
}